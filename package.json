{
  "name": "node-express-ts-ddd",
  "version": "1.0.0",
  "description": "beatifully template",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jhon-birreo/node-express-ts-ddd.git"
  },
  "author": "Jhon Birreo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jhon-birreo/node-express-ts-ddd/issues"
  },
  "homepage": "https://github.com/jhon-birreo/node-express-ts-ddd#readme",
  "scripts": {
    "build": "tsc",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "tsc && node build/server.js",
    "dev": "ts-node-dev --respawn -- src/server.ts",
    "studio": "prisma studio",
    "prepare": "husky install",
    "lint": "eslint src/**/*.ts",
    "format": "eslint src/**/*.ts --fix",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write"
  },
  "dependencies": {
    "@prisma/client": "^4.16.1",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.1.4",
    "express": "^4.18.2",
    "express-response-formatter": "^2.0.2",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "reflect-metadata": "^0.1.13",
    "ulid": "^2.3.0",
    "uuid": "^9.0.0",
    "uuid-validate": "^0.0.3"
  },
  "devDependencies": {
    "@ngneat/falso": "^6.4.0",
    "@types/bcrypt": "^5.0.0",
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.3.1",
    "@types/uuid": "^9.0.2",
    "@types/uuid-validate": "^0.0.1",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "concurrently": "^8.2.0",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "jest": "^29.6.1",
    "lint-staged": "^13.2.3",
    "prettier": "^3.0.0",
    "prisma": "^4.16.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.3"
  },
  "lint-staged": {
    "**/*.{ts}": [
      "npx prettier --write",
      "npx eslint --fix"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "test",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "moduleNameMapper": {
      "^@/lambdas(.*)$": "<rootDir>/lambdas$1",
      "^@/modules(.*)$": "<rootDir>/modules$1",
      "^@/shared(.*)$": "<rootDir>/shared$1"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
